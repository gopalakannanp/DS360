	public String Create_PO(String Order, String rec, String comm, String serial1, String serial2, String serial3,
			String serial4, String serial5, String serial6, String serial7, String serial8, String serial9,
			String serial10, String serial11, String serial12, String serial13, String serial14, String serial15,
			String serial16, String serial17, String serial18, String serial19, String serial20, String serial21,
			String serial22, String serial23, String serial24, String serial25, String serial26, String serial27,
			String serial28) throws InterruptedException {


		// driver.get("https://ktds.diveshopx.com/1.9.0/reverse_purchase_order");

		Thread.sleep(3000);

		waitUntilElementClickable(Vendordropdown, 30);

		WebElement get = Innertext;

		List<WebElement> option = get.findElements(By.xpath("//*[@id='search_vendor_chosen']/div/ul/li"));

		for (WebElement options : option) {

			String sta = options.getAttribute("innerHTML"); // Drop down click

			if (sta.contentEquals("VENDORSAMSUNG")) {

				options.click();
				break;
			}
		}

		Thread.sleep(2000);

		searchpart.click();

		Thread.sleep(2000);

		switchWindow();
		// switchtowindow();

		Thread.sleep(3000);

		waitUntilElementClickable(searchbtn, 30);

		Thread.sleep(2000);

		checkbox_selectall.click();

		Addselectedtopobtn.click();

		// switchtoparentwindow();
		switchWindow();
		Order_Qty1.clear();

		Order_Qty1.sendKeys(Order);

		Receive_Qty1.clear();

		Receive_Qty1.sendKeys(rec);

		Order_Qty2.clear();

		Order_Qty2.sendKeys(Order);

		Receive_Qty2.clear();

		Receive_Qty2.sendKeys(rec);

		Order_Qty3.clear();

		Order_Qty3.sendKeys(Order);

		Receive_Qty3.clear();

		Receive_Qty3.sendKeys(rec);

		Order_Qty4.clear();

		Order_Qty4.sendKeys(Order);

		Receive_Qty4.clear();

		Receive_Qty4.sendKeys(rec);

		Order_Qty5.clear();

		Order_Qty5.sendKeys(Order);

		Receive_Qty5.clear();

		Receive_Qty5.sendKeys(rec);

		Order_Qty6.clear();

		Order_Qty6.sendKeys(Order);

		Receive_Qty6.clear();

		Receive_Qty6.sendKeys(rec);

		Order_Qty7.clear();

		Order_Qty7.sendKeys(Order);

		Receive_Qty7.clear();

		Receive_Qty7.sendKeys(rec);

		commentfield.sendKeys(comm);

		Vendorinvoice.sendKeys("12");

		Receive_Goods_btn.click();

		Thread.sleep(3000);

		Serial1.sendKeys(serial1);

		Serial2.sendKeys(serial2);

		Serial3.sendKeys(serial3);

		Serial4.sendKeys(serial4);

		Serial5.sendKeys(serial5);

		Serial16.sendKeys(serial6);

		Serial7.sendKeys(serial7);

		Serial8.sendKeys(serial8);

		Serial9.sendKeys(serial9);

		Serial10.sendKeys(serial10);

		Serial11.sendKeys(serial11);

		Serial12.sendKeys(serial12);

		Serial13.sendKeys(serial13);

		Serial14.sendKeys(serial14);

		Serial15.sendKeys(serial15);

		Serial16.sendKeys(serial16);

		Serial17.sendKeys(serial17);

		Serial18.sendKeys(serial18);

		Serial19.sendKeys(serial19);

		Serial20.sendKeys(serial20);

		Serial21.sendKeys(serial21);

		Serial22.sendKeys(serial22);

		Serial23.sendKeys(serial23);

		Serial24.sendKeys(serial24);

		Serial25.sendKeys(serial25);

		Serial26.sendKeys(serial26);

		Serial27.sendKeys(serial27);

		Serial28.sendKeys(serial28);

		ADD.click();

		Thread.sleep(3000);

		String text = Alertmsg.getText();

		System.out.println(text);

		OK_btn.click();

		return text;

	}

